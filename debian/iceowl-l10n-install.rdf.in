<?xml version="1.0"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">
  <Description about="urn:mozilla:install-manifest"
               em:id="langpack-##LOCALE##@iceowl.debian.org"
               em:name="##LOCALE## Language Pack for Iceowl"
               em:version="##VERSION##"
               em:type="8"
               em:creator="debian.org">
    <!-- Sunbird/Iceowl
         Sunbird is no longer maintained by Mozilla
         see also ROM #742946 for Iceowl
    <em:targetApplication>
       <Description>
         <em:id>{718e30fb-e89b-41dd-9da7-e25a45638b28}</em:id>
         <em:minVersion>1.9</em:minVersion>
         <em:maxVersion>1.9.*</em:maxVersion>
       </Description>
    </em:targetApplication>
    <em:appManaged>true</em:appManaged>
    -->

    <!-- Thunderbird/Icedove -->
    <em:targetApplication>
      <Description>
        <em:id>{3550f703-e582-4d05-9a08-453d09bdfdc6}</em:id>
        <em:minVersion>45.0</em:minVersion>
        <em:maxVersion>45.*</em:maxVersion>
      </Description>
    </em:targetApplication>
    <em:appManaged>true</em:appManaged>

    <!-- Seamonkey/Iceape
         Iceape is dropped out because of no availability for security updates.
         see #732314 and https://www.debian.org/security/2013/dsa-2819
    <em:targetApplication>
      <Description>
        <em:id>{92650c4d-4b8e-4d2a-b7eb-24ecf4f6b63a}</em:id>
        <em:minVersion>2.21</em:minVersion>
        <em:maxVersion>2.21.*</em:maxVersion>
      </Description>
    </em:targetApplication>
    <em:appManaged>true</em:appManaged>
    -->
  </Description>
</RDF>
